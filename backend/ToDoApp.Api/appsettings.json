{
  "MongoDb": {
    "DatabaseName": "todoapp"
  },
  "TaskValidation": {
    "MaximumTitleLength": 100,
    "MaximumDescriptionLength": 2000
  },
  "Keycloak": {
    "Audience": "todoapi",
    "SwaggerClientId": "todoapi-swagger"
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Elasticsearch"
      }
    ]
  }
}
